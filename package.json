{
  "name": "react-spa",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run watch-css & REACT_APP_MODE=dev react-scripts start",
    "build": "npm run build-css && REACT_APP_MODE=dev react-scripts build",
    "watch-css": "node-sass --recursive src/styles/sass -o src/styles -w",
    "build-css": "node-sass --recursive src/styles/sass -o src/styles",
    "test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "api-book": "^0.1.0",
    "babel-polyfill": "^6.22.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.3.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "react-scripts": "0.8.5",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-thunk": "^2.2.0"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yingray/react-spa.git"
  },
  "author": "yingray",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yingray/react-spa/issues"
  }
}
